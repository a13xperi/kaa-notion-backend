{
  "name": "kaa-notion-integration",
  "version": "1.0.0",
  "description": "KAA Notion workspace integration with Cursor",
  "scripts": {
    "start": "node notion-api-server-enhanced.js",
    "build": "echo 'No build required for API server'",
    "dev": "concurrently \"npm run start\" \"cd kaa-app && npm start\"",
    "install-all": "npm install && cd kaa-app && npm install",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:seed": "prisma db seed"
  },
  "dependencies": {
    "@notionhq/client": "^5.1.0",
    "@prisma/client": "^7.2.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "form-data": "^4.0.4",
    "multer": "^2.0.2",
    "node-fetch": "^2.7.0",
    "nodemailer": "^7.0.7",
    "openai": "^4.104.0",
    "stripe": "^17.5.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "prisma": "^5.22.0"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  }
}
