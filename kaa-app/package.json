{
  "name": "kaa-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@notionhq/client": "^5.1.0",
    "@tanstack/react-query": "^5.90.16",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "@types/react": "^18.2.78",
    "@types/react-dom": "^18.2.25",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.30.3",
    "react-scripts": "5.0.1",
    "typescript": "^5.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "dev": "concurrently -k -n api,web -c green,blue \"npm run dev:api\" \"npm run dev:web\"",
    "dev:web": "react-scripts start",
    "dev:api": "npm --prefix ../server run dev",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "eject": "react-scripts eject",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:codegen": "playwright codegen localhost:3000",
    "stop": "sh -c 'PIDS=$(lsof -ti tcp:3000,3001,3002 || true); [ -n \"$PIDS\" ] && kill -TERM $PIDS || true'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@storybook/addon-essentials": "^7.6.21",
    "@storybook/react": "^7.6.21",
    "@storybook/react-webpack5": "^7.6.21",
    "concurrently": "^8.2.2",
    "storybook": "^7.6.21"
  }
}
